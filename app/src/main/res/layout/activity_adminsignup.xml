<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/c2"
    tools:context=".Adminsignup">

    <TextView
        android:id="@+id/textViewadminsignupid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Admin SignuP"
        android:textColor="@color/black"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.16000003" />

    <EditText
        android:id="@+id/editTextfirstnameid"
        android:layout_width="254dp"
        android:layout_height="37dp"
        android:background="@drawable/inputblack"
        android:ems="10"
        android:hint="Enter your first name"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewadminsignupid"
        app:layout_constraintVertical_bias="0.145"
        android:gravity="center_horizontal" />

    <EditText
        android:id="@+id/editTextlastnameid"
        android:layout_width="254dp"
        android:layout_height="37dp"
        android:background="@drawable/inputblack"
        android:ems="10"
        android:hint="Enter your Last name"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextfirstnameid"
        app:layout_constraintVertical_bias="0.060000002"
        android:gravity="center_horizontal" />

    <EditText
        android:id="@+id/editTextemailid"
        android:layout_width="254dp"
        android:layout_height="37dp"
        android:background="@drawable/inputblack"
        android:ems="10"
        android:hint="abc@gmail.com"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:textAlignment="center"
        android:paddingBottom="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextlastnameid"
        app:layout_constraintVertical_bias="0.061"
        android:gravity="center_horizontal" />

    <EditText
        android:id="@+id/editTextphonenumberid"
        android:layout_width="230dp"
        android:layout_height="37dp"
        android:background="@drawable/inputblack"
        android:ems="10"
        android:hint="01777777777"
        android:inputType="number"
        android:textAlignment="center"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextemailid"
        app:layout_constraintVertical_bias="0.060000002"
        android:gravity="center_horizontal" />

    <EditText
        android:id="@+id/editTextpasswordid"
        android:layout_width="200dp"
        android:layout_height="37dp"
        android:background="@drawable/inputblack"
        android:ems="10"
        android:hint="**********"
        android:inputType="textPassword"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextphonenumberid"
        app:layout_constraintVertical_bias="0.060000002"
        android:gravity="center_horizontal" />

    <Button
        android:id="@+id/buttonadminsignupid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="signup"
        app:backgroundTint="@color/teal_700"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextpasswordid"
        app:layout_constraintVertical_bias="0.26" />

    <TextView
        android:id="@+id/textViewallreadyregisterid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="All ready register? Login"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonadminsignupid"
        app:layout_constraintVertical_bias="0.13" />

    <ProgressBar
        android:id="@+id/progressBaradminsignid"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="@+id/buttonadminsignupid"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewadminsignupid"
        app:layout_constraintVertical_bias="0.050000012" />

    <ImageView
        android:id="@+id/imageViewsignupbackid"
        android:layout_width="96dp"
        android:layout_height="52dp"
        app:layout_constraintBottom_toTopOf="@+id/textViewadminsignupid"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.13"
        app:srcCompat="@drawable/b1" />
</androidx.constraintlayout.widget.ConstraintLayout>
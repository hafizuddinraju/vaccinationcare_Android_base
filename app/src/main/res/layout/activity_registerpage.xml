<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/c4"
    tools:context=".Registerpage">

    <TextView
        android:id="@+id/textViewusersignupid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Signup"
        android:textColor="@color/black"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.103" />

    <EditText
        android:id="@+id/editTextuseremailid"
        android:layout_width="245dp"
        android:layout_height="44dp"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginEnd="5dp"
        android:layout_marginRight="5dp"
        android:background="@drawable/input"
        android:ems="10"
        android:gravity="center_horizontal"
        android:hint="abc@gmail.com"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"

        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewusersignupid"
        app:layout_constraintVertical_bias="0.01999998" />

    <EditText
        android:id="@+id/editTextusernameid"
        android:layout_width="245dp"
        android:layout_height="44dp"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="5dp"
        android:layout_marginRight="5dp"
        android:background="@drawable/input"
        android:ems="10"
        android:gravity="center_horizontal"
        android:hint="Enter user Name"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextuseremailid"
        app:layout_constraintVertical_bias="0.04000002" />


    <EditText
        android:id="@+id/editTextuserdateofbirthid"
        android:layout_width="163dp"
        android:layout_height="38dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/input"
        android:ems="10"
        android:gravity="center_horizontal"
        android:hint="dd/mm/yyyy"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.334"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextusernameid"
        app:layout_constraintVertical_bias="0.04000002" />


    <EditText
        android:id="@+id/editTextuserphonenumberid"
        android:layout_width="245dp"
        android:layout_height="44dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="288dp"
        android:background="@drawable/input"
        android:ems="10"
        android:gravity="center_horizontal"
        android:hint="Enter your phone number"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextuserdateofbirthid"
        app:layout_constraintVertical_bias="0.07999998" />

    <EditText
        android:id="@+id/editTextuserpasswordid"
        android:layout_width="245dp"
        android:layout_height="44dp"
        android:background="@drawable/input"
        android:ems="10"
        android:gravity="center_horizontal"
        android:hint="Enter your password"
        android:inputType="textPassword"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextuserphonenumberid"
        app:layout_constraintVertical_bias="0.04000002" />

    <EditText
        android:id="@+id/editTextuserconfirmpasswordid"
        android:layout_width="245dp"
        android:layout_height="44dp"
        android:background="@drawable/input"
        android:ems="10"
        android:gravity="center_horizontal"
        android:hint="Confirm password"
        android:inputType="textPassword"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextuserpasswordid"
        app:layout_constraintVertical_bias="0.04000002" />

    <Button
        android:id="@+id/buttonusersignupid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Register"
        app:backgroundTint="#05104A"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextuserconfirmpasswordid"
        app:layout_constraintVertical_bias="0.208" />

    <TextView
        android:id="@+id/textViewuserloginid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="all ready register? login"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonusersignupid"
        app:layout_constraintVertical_bias="0.16000003" />

    <ProgressBar
        android:id="@+id/progressBar"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.9" />

    <ImageView
        android:id="@+id/imageViewusercalid"
        android:layout_width="37dp"
        android:layout_height="38dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.26"
        app:layout_constraintStart_toEndOf="@+id/editTextuserdateofbirthid"
        app:layout_constraintTop_toBottomOf="@+id/editTextusernameid"
        app:layout_constraintVertical_bias="0.060000002"
        app:srcCompat="@drawable/calendar" />

    <ImageView
        android:id="@+id/imageViewuserbackid"
        android:layout_width="94dp"
        android:layout_height="54dp"
        app:layout_constraintBottom_toTopOf="@+id/textViewusersignupid"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.17000002"
        app:srcCompat="@drawable/b1" />


</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/c4"
    tools:context=".Myvaccination">

    <TextView
        android:id="@+id/textViewvaccine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="VACCINE"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.110000014" />

    <EditText
        android:id="@+id/editTextEmailAddressid"
        android:layout_width="256dp"
        android:layout_height="39dp"
        android:background="@drawable/input"
        android:ems="10"
        android:hint="Enter your phone number"
        android:inputType="number"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textColor="@color/black"
        android:textColorHint="#687ADF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewvaccine"
        app:layout_constraintVertical_bias="0.06999999" />

    <EditText
        android:id="@+id/editTextvaccinenameid"
        android:layout_width="256dp"
        android:layout_height="39dp"
        android:background="@drawable/input"
        android:ems="10"

        android:hint="Enter your vaccine name"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textColor="@color/black"
        android:textColorHint="#687ADF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextEmailAddressid"
        app:layout_constraintVertical_bias="0.060000002" />

    <EditText
        android:id="@+id/editTextvaccinestartid"
        android:layout_width="188dp"
        android:layout_height="39dp"
        android:background="@drawable/input"
        android:ems="10"
        android:hint="Start:dd/mm/yyyy"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textColor="@color/black"
        android:textColorHint="#687ADF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.34"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextvaccinenameid"
        app:layout_constraintVertical_bias="0.059" />

    <ImageView
        android:id="@+id/imageViewstartdateid"
        android:layout_width="45dp"
        android:layout_height="37dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.245"
        app:layout_constraintStart_toEndOf="@+id/editTextvaccinestartid"
        app:layout_constraintTop_toBottomOf="@+id/editTextvaccinenameid"
        app:layout_constraintVertical_bias="0.063"
        app:srcCompat="@drawable/calendar" />

    <EditText
        android:id="@+id/editTextNumberofdoseid"
        android:layout_width="256dp"
        android:layout_height="39dp"
        android:background="@drawable/input"
        android:ems="10"
        android:hint="Enter your vaccine dose "
        android:inputType="number"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textColor="@color/black"
        android:textColorHint="#687ADF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextvaccinestartid"
        app:layout_constraintVertical_bias="0.060000002" />

    <EditText
        android:id="@+id/editTextpriceid"
        android:layout_width="256dp"
        android:layout_height="39dp"
        android:background="@drawable/input"
        android:ems="10"
        android:hint="Enter your vaccine price"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textColor="@color/black"
        android:textColorHint="#687ADF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextNumberofdoseid"
        app:layout_constraintVertical_bias="0.063" />

    <EditText
        android:id="@+id/editTextplaceid"
        android:layout_width="250dp"
        android:layout_height="66dp"
        android:background="@drawable/input"
        android:ems="10"
        android:hint="Enter your place"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textColor="@color/black"
        android:textColorHint="#687ADF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextpriceid"
        app:layout_constraintVertical_bias="0.06" />

    <Button
        android:id="@+id/buttonupdateid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"

        android:text="UPDATE"
        android:backgroundTint="#05104A"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextplaceid"
        app:layout_constraintVertical_bias="0.26" />

    <Button
        android:id="@+id/buttonupdatelogoutid"
        android:layout_width="93dp"
        android:layout_height="36dp"
        android:background="@color/white"
        android:backgroundTint="#05104A"
        android:text="logout"
        android:textSize="12sp"
        app:backgroundTint="#05104A"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.898"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonupdateid" />

    <Button
        android:id="@+id/buttonupdatebackid"
        android:layout_width="99dp"
        android:layout_height="45dp"
        android:text="Back"

        app:backgroundTint="#05104A"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/buttonupdatelogoutid"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonupdateid" />

</androidx.constraintlayout.widget.ConstraintLayout>